{% set generator_gh_repo = generator_namespace ~ "/ansible-role-" ~ generator_role_name %}
{% set generator_gh_repo_url = "https://github.com/" ~ generator_gh_repo %}
{% set generator_srht_repo = "~" ~ generator_namespace ~ "/ansible-role-" ~ generator_role_name %}
{% set generator_srht_repo_url = "https://git.sr.ht/" ~ generator_srht_repo %}
{% set generator_ci_molecule_url = generator_gh_repo_url ~ "/actions/workflows/molecule.yml" %}
{% set generator_ci_galaxy_url= generator_gh_repo_url ~ "/actions/workflows/galaxy.yml" %}
{% set generator_galaxy_id = _generator_galaxy_id.stdout | default('unset') %}
[badge-role]: https://img.shields.io/ansible/role/{{ generator_galaxy_id }}.svg?logo=ansible
[link-galaxy]: https://galaxy.ansible.com/{{ generator_namespace }}/{{ generator_role_name }}
[badge-github-repo]: https://img.shields.io/static/v1?label=GitHub&message=repo&color=blue&logo=github
[link-github-repo]: {{ generator_gh_repo_url }}
[badge-srht-repo]: https://img.shields.io/static/v1?label=SourceHut&message=repo&color=blue&logo=data:image/svg+xml;base64,
[link-srht-repo]: {{ generator_srht_repo_url }}
[badge-license]: https://img.shields.io/github/license/{{ generator_gh_repo }}.svg?logo=github
[link-license]: {{ generator_gh_repo_url }}/blob/main/LICENSE
[badge-github-issues]: https://img.shields.io/github/issues/{{ generator_gh_repo }}.svg?logo=github
[link-github-issues]: {{ generator_gh_repo_url }}/issues
[badge-github-prs]: https://img.shields.io/github/issues-pr/{{ generator_gh_repo }}.svg?logo=github
[link-github-prs]: {{ generator_gh_repo_url }}/pulls
[badge-version]: https://img.shields.io/github/release/{{ generator_gh_repo }}.svg?logo=github
[link-version]: {{ generator_gh_repo_url }}/releases/latest
[badge-commits-since]: https://img.shields.io/github/commits-since/{{ generator_gh_repo }}/latest.svg?logo=github
[badge-quality]: https://img.shields.io/ansible/quality/{{ generator_galaxy_id }}.svg?logo=ansible
[badge-downloads]: https://img.shields.io/ansible/role/d/{{ generator_galaxy_id }}.svg?logo=ansible
[badge-molecule-workflow]: {{ generator_ci_molecule_url }}/badge.svg?branch=main
[link-molecule-workflow]: {{ generator_ci_molecule_url }}
[badge-galaxy-workflow]: {{ generator_ci_galaxy_url }}/badge.svg
[link-galaxy-workflow]: {{ generator_ci_galaxy_url }}
[link-defaults]: {{ generator_gh_repo_url }}/blob/main/defaults/main.yml
